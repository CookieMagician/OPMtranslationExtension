{
    "manifest_version": 2,
    "name": "Tonarinoyj Translation Applicator",
    "version": "2.0",
    "description": "Applies transleted text over opm chapters",
    "options_page": "options.html",

    "browser_action": {
      "default_icon": "icon32.png",
      "default_popup": "popup.html"
    },



     "background": {
       "scripts": ["jquery-3.5.1.min.js","background.js"],
       "persistent": false
     },


    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

    "permissions": [
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/",
    "storage"
  ],

  "browser_specific_settings": {
    "gecko": {
    "id": "TonarinoyjTranslationApplicator@lol.com",
    "strict_min_version": "42.0"
    }
  },


  "options_ui": {
  "page": "options.html"

  }
}
