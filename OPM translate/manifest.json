{
    "manifest_version": 2,
    "name": "OPM translator",
    "version": "1.0",
    "description": "Applies transleted text over opm chapters",
    "options_page": "options.html",

    "browser_action": {
      "default_icon": "icon32.png",
      "default_popup": "popup.html"
    },

    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

    "permissions": [
    "tabs",
    "http://*/",
    "https://*/",
    "storage"
  ]


}
